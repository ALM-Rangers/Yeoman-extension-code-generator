{
  "manifestVersion": 1,
  "id": "<%= ExtensionID %>",
  "version": "0.1.0",
  "name": "<%= ExtensionName %>",
  "scopes": [],
  "description": "<%= Description %>",
  "publisher": "<%= PublisherId %>",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Sample"
  ],
  "categories": [
    "Integrate"
  ],
  "screenshots": [
    {
      "path": "static/images/screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "license.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://bit.ly"
    },
    "support": {
      "uri": "https://bit.ly"
    },
    "issues": {
      "uri": "https://bit.ly"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://bit.ly"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "contributions": [
    {
      "id": "<%= consumername %>",
      "type": "ms.vss-servicehooks.consumer",
      "targets": [
        "ms.vss-servicehooks.consumers"
      ],
      "properties": {
        "id": "<%= consumername %>",
        "name": "<%= friendlyName %>",
        "description": "<%= ServiceDescription %>",
        "informationUrl": "https://aka.ms/vsoextensions",
        "inputDescriptors": [
          {
            "id": "url",
            "isRequired": true,
            "name": "URL",
            "description": "URL to post event payload to",
            "inputMode": "textbox"
          }
        ],
        "actions": [
         <%- events %>
        ]
      }
    }
  ]
}